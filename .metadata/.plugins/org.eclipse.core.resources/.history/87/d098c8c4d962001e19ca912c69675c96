package Lab2_1;

import java.util.ArrayList;
import java.util.Collections;
import java.util.List;
import java.util.Map;

public class NodeUtils {
	public static String printPath(Node current, Map<Integer, Integer> parents) {
		int value = current.getState();
		String s = value + "";
		// value = parent.get(value) => parent
		while ((value = parents.getOrDefault(value, -1))!= -1) {
			s = value +" " + s;
		}
		return s;
	}
}
